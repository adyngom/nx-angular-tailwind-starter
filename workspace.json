{
  "version": 2,
  "projects": {
    "tukki": {
      "projectType": "application",
      "root": "apps/tukki",
      "sourceRoot": "apps/tukki/src",
      "prefix": "tukki",
      "targets": {
        "build": {
          "executor": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/apps/tukki",
            "index": "apps/tukki/src/index.html",
            "main": "apps/tukki/src/main.ts",
            "polyfills": "apps/tukki/src/polyfills.ts",
            "tsConfig": "apps/tukki/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/tukki/src/favicon.ico",
              "apps/tukki/src/assets"
            ],
            "styles": [
              "apps/tukki/src/styles.scss"
            ],
            "scripts": [],
            "customWebpackConfig": {
              "path": "webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/tukki/src/environments/environment.ts",
                  "with": "apps/tukki/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "tukki:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "tukki:build:production"
            }
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tukki:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/tukki/src/**/*.ts",
              "apps/tukki/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/tukki"
          ],
          "options": {
            "jestConfig": "apps/tukki/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "tukki-e2e": {
      "root": "apps/tukki-e2e",
      "sourceRoot": "apps/tukki-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/tukki-e2e/cypress.json",
            "tsConfig": "apps/tukki-e2e/tsconfig.e2e.json",
            "devServerTarget": "tukki:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tukki:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/tukki-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "shared-ui-header": {
      "projectType": "library",
      "root": "libs/shared/ui/header",
      "sourceRoot": "libs/shared/ui/header/src",
      "prefix": "tukki",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui/header/src/**/*.ts",
              "libs/shared/ui/header/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared/ui/header"
          ],
          "options": {
            "jestConfig": "libs/shared/ui/header/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-ui-hero": {
      "projectType": "library",
      "root": "libs/shared/ui/hero",
      "sourceRoot": "libs/shared/ui/hero/src",
      "prefix": "tukki",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui/hero/src/**/*.ts",
              "libs/shared/ui/hero/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared/ui/hero"
          ],
          "options": {
            "jestConfig": "libs/shared/ui/hero/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-ui-promo": {
      "projectType": "library",
      "root": "libs/shared/ui/promo",
      "sourceRoot": "libs/shared/ui/promo/src",
      "prefix": "tukki",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui/promo/src/**/*.ts",
              "libs/shared/ui/promo/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared/ui/promo"
          ],
          "options": {
            "jestConfig": "libs/shared/ui/promo/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-ui-partials": {
      "projectType": "library",
      "root": "libs/shared/ui/partials",
      "sourceRoot": "libs/shared/ui/partials/src",
      "prefix": "tukki",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui/partials/src/**/*.ts",
              "libs/shared/ui/partials/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared/ui/partials"
          ],
          "options": {
            "jestConfig": "libs/shared/ui/partials/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-ui-footer": {
      "projectType": "library",
      "root": "libs/shared/ui/footer",
      "sourceRoot": "libs/shared/ui/footer/src",
      "prefix": "tukki",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui/footer/src/**/*.ts",
              "libs/shared/ui/footer/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared/ui/footer"
          ],
          "options": {
            "jestConfig": "libs/shared/ui/footer/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "terru": {
      "root": "apps/terru",
      "sourceRoot": "apps/terru/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/terru",
            "index": "apps/terru/src/index.html",
            "main": "apps/terru/src/main.tsx",
            "polyfills": "apps/terru/src/polyfills.ts",
            "tsConfig": "apps/terru/tsconfig.app.json",
            "assets": [
              "apps/terru/src/favicon.ico",
              "apps/terru/src/assets"
            ],
            "styles": [
              "apps/terru/src/styles.scss",
              "apps/terru/src/tailwind.css"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/terru/src/environments/environment.ts",
                  "with": "apps/terru/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "build-tailwind-css": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node node_modules/.bin/postcss apps/terru/src/styles.scss -o apps/terru/src/app/tailwind.css",
            "cwd": "apps/terru"
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "terru:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "terru:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/terru/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/terru"
          ],
          "options": {
            "jestConfig": "apps/terru/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "terru-e2e": {
      "root": "apps/terru-e2e",
      "sourceRoot": "apps/terru-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/terru-e2e/cypress.json",
            "tsConfig": "apps/terru-e2e/tsconfig.e2e.json",
            "devServerTarget": "terru:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "terru:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/terru-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "tukki"
}
