{
  "version": 1,
  "cli": {
    "analytics": "71c04e7b-554a-489b-bec3-f437c975b324",
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "defaultProject": "tukki",
  "projects": {
    "tukki": {
      "projectType": "application",
      "root": "apps/tukki",
      "sourceRoot": "apps/tukki/src",
      "prefix": "tukki",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/apps/tukki",
            "index": "apps/tukki/src/index.html",
            "main": "apps/tukki/src/main.ts",
            "polyfills": "apps/tukki/src/polyfills.ts",
            "tsConfig": "apps/tukki/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/tukki/src/favicon.ico", "apps/tukki/src/assets"],
            "styles": ["apps/tukki/src/styles.css"],
            "scripts": [],
            "customWebpackConfig": {
              "path": "webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/tukki/src/environments/environment.ts",
                  "with": "apps/tukki/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "tukki:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "tukki:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tukki:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/tukki/src/**/*.ts",
              "apps/tukki/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/tukki"],
          "options": {
            "jestConfig": "apps/tukki/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "tukki-e2e": {
      "root": "apps/tukki-e2e",
      "sourceRoot": "apps/tukki-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/tukki-e2e/cypress.json",
            "tsConfig": "apps/tukki-e2e/tsconfig.e2e.json",
            "devServerTarget": "tukki:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tukki:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/tukki-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "shared-ui-header": {
      "projectType": "library",
      "root": "libs/shared/ui/header",
      "sourceRoot": "libs/shared/ui/header/src",
      "prefix": "tukki",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui/header/src/**/*.ts",
              "libs/shared/ui/header/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/ui/header"],
          "options": {
            "jestConfig": "libs/shared/ui/header/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-ui-hero": {
      "projectType": "library",
      "root": "libs/shared/ui/hero",
      "sourceRoot": "libs/shared/ui/hero/src",
      "prefix": "tukki",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui/hero/src/**/*.ts",
              "libs/shared/ui/hero/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/ui/hero"],
          "options": {
            "jestConfig": "libs/shared/ui/hero/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-ui-promo": {
      "projectType": "library",
      "root": "libs/shared/ui/promo",
      "sourceRoot": "libs/shared/ui/promo/src",
      "prefix": "tukki",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui/promo/src/**/*.ts",
              "libs/shared/ui/promo/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/ui/promo"],
          "options": {
            "jestConfig": "libs/shared/ui/promo/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-ui-partials": {
      "projectType": "library",
      "root": "libs/shared/ui/partials",
      "sourceRoot": "libs/shared/ui/partials/src",
      "prefix": "tukki",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui/partials/src/**/*.ts",
              "libs/shared/ui/partials/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/ui/partials"],
          "options": {
            "jestConfig": "libs/shared/ui/partials/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-ui-footer": {
      "projectType": "library",
      "root": "libs/shared/ui/footer",
      "sourceRoot": "libs/shared/ui/footer/src",
      "prefix": "tukki",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui/footer/src/**/*.ts",
              "libs/shared/ui/footer/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/ui/footer"],
          "options": {
            "jestConfig": "libs/shared/ui/footer/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
